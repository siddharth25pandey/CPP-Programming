#include <iostream>
using namespace std;

int main()
{
    int expo;
    float base, ans=1;

    cout << "Enter base and exponent respectively:  ";
    cin >> base >> expo;

    cout << base << "^" << expo << " = ";

    while (expo != 0) {
        ans *= base;
        --expo;
    }

    cout << ans;

    return 0;
}
